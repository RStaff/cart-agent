{
  "name": "shopify-app-template-node",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
  "dev": "cross-env NODE_ENV=development nodemon --config nodemon.json index.js",
  "dev:kill": "bash scripts/dev-kill.sh",
  "dev:reset": "bash scripts/dev-reset.sh",
  "seed:merchant": "SEED_SHOP=example.com SEED_KEY=dev_api_key_123 node --env-file=.env seed-merchant.js",
  "seed:cart": "curl -s -X POST http://localhost:3000/ingest/cart -H 'Content-Type: application/json' -H 'X-Api-Key: dev_api_key_123' -H 'X-Shop-Domain: example.com' --data @scripts/cart.json",
  "copy:generate": "API_KEY=dev_api_key_123 SHOP=example.com CART_ID=CART-001 VARIANT=subject_a node -e \"fetch('http://localhost:3000/copy/generate',{method:'POST',headers:{'Content-Type':'application/json','X-Api-Key':process.env.API_KEY,'X-Shop-Domain':process.env.SHOP},body:JSON.stringify({cartId:process.env.CART_ID,variant:process.env.VARIANT})}).then(r=>r.text()).then(t=>console.log(t)).catch(console.error)\""
}

  },
  "type": "module",
  "engines": {
    "node": ">=16.13.0"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@sendgrid/mail": "^8.1.5",
    "@shopify/shopify-app-express": "^5.0.8",
    "@shopify/shopify-app-session-storage-sqlite": "^4.0.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "resend": "^6.0.1",
    "serve-static": "^1.14.1",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "prisma": "^6.13.0"
  }
}
