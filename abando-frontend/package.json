{
  "name": "abando-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --cache",
    "typecheck": "tsc --noEmit",
    "check:all": "npm run lint && npm run typecheck",
    "next:lint": "next lint",
    "start:prod": "scripts/start-prod.sh",
    "stop:prod": "scripts/stop-prod.sh",
    "status:prod": "scripts/status-prod.sh",
    "smoke": "node scripts/smoke-trial.mjs",
    "build:ci": "next build",
    "guard:stripe": "scripts/stripe-env-check.sh",
    "prebuild": "scripts/guard-next-navigation.sh && scripts/guard-searchparams.sh && sh -c 'npm run guard:autosend && true'",
    "guard:nextnav": "scripts/guard-next-navigation.sh",
    "guard:autosend": "scripts/autosend-guard.sh"
  },
  "dependencies": {
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "5.9.2"
  },
  "type": "module"
}
